/**
 * @param {number[]} nums
 * @return {number[]}
 */
var sortArray = function(nums) {
    for(let i = 0; i < nums.length; i++) {
        let lowest = i;
        for(let j = i+1; j < nums.length; j++) {
            if(nums[lowest] > nums[j]) {
                lowest = j;
            }
        }
        if(lowest !== i) {
            [nums[i], nums[lowest]] = [nums[lowest], nums[i]];
        }
    }
    return nums;
};
